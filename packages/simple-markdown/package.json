        "README.md"
    ],
    "dependencies": {
        "@types/react": ">=16.0.0"
    },
    "devDependencies": {
        "@types/react-dom": ">=16.0.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "rollup": "^1.26.3",
